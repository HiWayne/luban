{
  "name": "luban",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev-frontend": "vite",
    "build-frontend": "tsc && vite build",
    "preview": "vite preview",
    "husky-prepare": "husky install",
    "test": "lint-staged --allow-empty && jest",
    "build-backend": "npx swc src/backend --only **/*.ts -d dist-backend --config-file .swcrc && node ./.sh/copyFolder.js ./asymmetricEncryptionKeys ./dist-backend/asymmetricEncryptionKeys && rm -rf ./dist-backend/_temp",
    "dev-backend": "npm run build-backend && node dist-backend/backend/index.js",
    "deploy": "rm -f dist.zip && npm run build-frontend && npm run build-backend && zip -r dist.zip ./.bin ./.sh ./dist-backend ./dist-frontend ./package.json ./pnpm-lock.yaml ecosystem.config.js",
    "start": "sh ./.bin/check_pm2_plugin.sh && pm2 start ecosystem.config.js",
    "pm2": "pm2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@babel/core": ">=7.0.0 <8.0.0",
    "@duitang/dt-base": "^2.1.15",
    "@duitang/dt-react-mobile": "^3.1.18",
    "@fastify/cors": "^8.2.0",
    "antd": "^5.0.2",
    "axios": "^1.2.1",
    "chalk": "^5.1.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "esbuild": "^0.16.1",
    "esbuild-plugin-less": "^1.1.11",
    "fastify": "^4.10.2",
    "immer": "^9.0.16",
    "jsencrypt": "^3.3.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mongodb": "^4.13.0",
    "node-rsa": "^1.1.1",
    "pm2": "^5.2.2",
    "prism-react-renderer": "^1.3.5",
    "qiankun": "^2.8.4",
    "rc-select": "^14.2.0",
    "rc-tween-one": "^3.0.6",
    "rc-virtual-list": "^3.4.13",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-simple-code-editor": "^0.13.1",
    "redis": "^4.5.1",
    "styled-components": "^5.3.6",
    "ws": "^8.11.0",
    "xss": "^1.0.14",
    "zustand": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-angular": "^17.3.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.20",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@types/node-rsa": "^1.1.1",
    "@types/react": "^18.0.24",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": ">=5.13.0 <6.0.0",
    "@typescript-eslint/parser": ">=5.0.0 <6.0.0",
    "@vitejs/plugin-react": "^3.0.1",
    "chokidar": "^3.5.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.4",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "typescript": "^4.6.4",
    "vite": "^4.0.4",
    "vite-plugin-svgr": "^2.4.0"
  },
  "engines": {
    "node": ">=17.0.0"
  },
  "lint-staged": {
    "*.js?(x)": [
      "eslint --fix"
    ],
    "*.{js,jsx,less,scss,sass,css,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**",
      "**/dist-front/**",
      "**/dist-backend/**"
    ],
    "delay": 1000
  }
}
